{
    "name": "sveltekit",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "noIndex": "vite build && echo 'User-agent: *\nDisallow: /' > build/robots.txt",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@fontsource/anonymous-pro": "^4.5.9",
        "@fontsource/plus-jakarta-sans": "^4.5.10",
        "@slidy/animation": "^1.0.7",
        "@slidy/core": "^3.7.4",
        "@slidy/easing": "^1.0.5",
		"@sveltejs/adapter-netlify": "^2.0.8",
        "@sveltejs/adapter-static": "^2.0.2",
        "@sveltejs/kit": "^1.22.3",
        "@types/three": "^0.148.0",
        "prettier": "^3.1.0",
        "prettier-plugin-svelte": "^3.1.2",
        "sass": "^1.56.2",
        "svelte": "^4.1.0",
        "svelte-check": "^3.4.6",
        "three": "^0.148.0",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0"
    },
    "type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.10.3",
		"@supabase/supabase-js": "^2.38.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte-forms-lib": "^2.0.1"
	}
}
