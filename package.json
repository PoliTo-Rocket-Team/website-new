{
	"name": "sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"noIndex": "vite build && echo 'User-agent: *\nDisallow: /' > build/robots.txt",
