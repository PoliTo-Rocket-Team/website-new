		"@sveltejs/kit": "^1.2.2",
		"@types/three": "^0.148.0",
		"sass": "^1.56.2",
		"svelte": "^3.59.1",
		"svelte-check": "^2.9.2",
		"three": "^0.148.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module"
}
